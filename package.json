{
  "name": "avynix-monorepo",
  "private": true,
  "version": "1.0.0",
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "dev:backend": "pnpm --filter avynix-streaming dev",
    "dev:streaming": "docker compose up mediamtx",
    "build:backend": "pnpm --filter avynix-streaming build",
    "dev": "concurrently -n STREAMING,NESTJS -c blue,magenta \"pnpm run dev:streaming\" \"pnpm run dev:backend\""
  },
  "devDependencies": {
    "concurrently": "^9.2.1"
  }
}
